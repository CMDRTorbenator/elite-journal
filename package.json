  },
  "devDependencies": {
    "devtron": "^1.3.0",
    "electron": "^1.4.1",
    "electron-builder": "^7.9.0",
    "electron-packager": "^8.0.0",
    "electron-zip-packager": "^4.0.0",
    "xo": "^0.16.0"
