{
	"non-interactive": false,
	"dry-run": false,
	"verbose": false,
	"force": false,
	"pkgFiles": ["package.json", "src/package.json"],
	"increment": "patch",
	"prereleaseId": null,
	"commitMessage": "v%s [build]",
	"tagName": "v%s",
	"tagAnnotation": "v%s",
	"buildCommand": false,

	"requireCleanWorkingDir": false,
	"src": {
		"pushRepo": null,
		"beforeStartCommand": false,
		"beforeStageCommand": false,
		"afterReleaseCommand": false
	},
	"dist": {
		"repo": false,
		"stageDir": ".stage",
		"baseDir": "dist",
		"files": ["**/*"],
		"pkgFiles": null,
		"beforeStageCommand": false,
		"afterReleaseCommand": false
	},
	"npm": {
		"publish": false,
		"publishPath": ".",
		"tag": "latest",
		"private": false,
		"forcePublishSourceRepo": false
	},
	"github": {
		"release": true,
		"releaseName": "Release %s",
		"tokenRef": "GITHUB_TOKEN"
	}
}
