{
	"non-interactive": false,
	"dry-run": false,
	"verbose": false,
	"force": false,
	"pkgFiles": ["package.json", "src/package.json"],
	"increment": "patch",
	"prereleaseId": null,
	"commitMessage": "v%s [build]",
	"tagName": "v%s",
	"tagAnnotation": "v%s",
	"buildCommand": false,

	"requireCleanWorkingDir": false,
	"src": {
		"pushRepo": true,
		"beforeStartCommand": "github-changes -o willyb321 -r elite-journal -k $GITHUB_TOKEN -b develop --use-commit-body && git add CHANGELOG.md",
		"beforeStageCommand": false,
		"afterReleaseCommand": false
	},
	"npm": {
		"publish": false,
		"publishPath": ".",
		"tag": "latest",
		"private": false,
		"forcePublishSourceRepo": false
	},
	"github": {
		"release": true,
		"releaseName": "Release %s",
		"tokenRef": "GITHUB_TOKEN"
	}
}
